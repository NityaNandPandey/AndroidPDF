<?xml version="1.0" encoding="utf-8"?>
<android.view.DispatchFairInsetsFrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    app:consumeInsets="false">

    <ViewStub
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/stub_progress"
        android:layout="@layout/controls_fragment_tabbed_sub_progress_view"/>

    <ViewStub
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/stub_pdfviewctrl"
        android:layout="@layout/controls_fragment_tabbed_sub_pdfviewctrl"/>

    <ViewStub
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/stub_password"
        android:layout="@layout/controls_fragment_tabbed_sub_password_view"/>

    <ViewStub
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/stub_reflow"
        android:layout="@layout/controls_fragment_tabbed_sub_reflow"/>

    <ViewStub
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/stub_overlay"
        android:layout="@layout/controls_fragment_tabbed_sub_overlay"/>

</android.view.DispatchFairInsetsFrameLayout>
