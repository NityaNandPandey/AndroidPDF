<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:id="@+id/selectable_shape"
        android:top="4dp"
          android:bottom="4dp"
          android:left="4dp"
          android:right="4dp">
        <shape android:shape="oval">
            <solid android:color="#ebd6bf"/>
            <stroke android:color="#8a000000"
                    android:width="1dp"/>
        </shape>
    </item>
    <item android:top="11dp"
          android:bottom="15dp"
          android:left="13dp"
          android:right="13dp">
        <bitmap android:src="@drawable/serif_a_letter_black"
                android:gravity="center"/>
    </item>
</layer-list>
