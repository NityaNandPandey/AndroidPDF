<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item
        android:id="@+id/selectable_shape"
        android:top="3dp"
        android:bottom="3dp"
        android:left="3dp"
        android:right="3dp">
        <shape android:shape="oval">
            <solid android:color="@android:color/white"/>
            <stroke android:color="#8a000000"
                android:width="1dp"/>
        </shape>
    </item>
    <item
        android:drawable="@drawable/ic_mode_day_black_24dp"
        android:top="10dp"
        android:bottom="10dp"
        android:left="10dp"
        android:right="10dp" />
</layer-list>
