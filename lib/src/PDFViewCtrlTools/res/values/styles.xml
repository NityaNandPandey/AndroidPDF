    <style name="CalloutPresetStyle1" parent="FreeTextPresetStyle1">
        <item name="annot_color">#e44234</item>
        <item name="annot_thickness">1</item>
    </style>

    <style name="CalloutPresetStyle2" parent="FreeTextPresetStyle2">
