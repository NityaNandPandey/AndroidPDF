<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="CustomAppTheme" parent="Theme.AppCompat.DayNight.NoActionBar"/>

    <style name="ToolbarTheme" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="ToolbarPopupTheme" parent="ThemeOverlay.AppCompat.Light" />

    <style name="transparent_button" parent="Widget.AppCompat.Button.Borderless">
        <item name="android:minWidth">@dimen/dialog_button_min_width</item>
        <item name="android:paddingLeft">@dimen/dialog_button_default_margin_padding</item>
        <item name="android:paddingRight">@dimen/dialog_button_default_margin_padding</item>
    </style>

    <style name="transparent_dialog_annotation_note_button" parent="Widget.AppCompat.Button.Borderless">
        <item name="android:minWidth">@dimen/dialog_button_min_width</item>
        <item name="android:paddingLeft">@dimen/dialog_button_default_margin_padding</item>
        <item name="android:paddingRight">@dimen/dialog_button_default_margin_padding</item>
        <item name="android:textColor">@color/button_dialog_annotation_note_color</item>
    </style>

    <style name="black_text_button" parent="Widget.AppCompat.Button.Borderless">
        <item name="android:textColor">@color/button_text_color</item>
    </style>

    <style name="fabMenuLabelStyle">
        <item name="android:background">@drawable/fab_menu_label_background</item>
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="StatusBarView">
        <!-- Fit system window insets by default, since the StatusBarView widget is
             only really useful when the fitsSystemWindows flag is set. -->
        <item name="android:fitsSystemWindows">true</item>
        <item name="android:background">?attr/colorPrimaryDark</item>
    </style>

    <style name="DispatchFairInsets">
        <!-- Fully consume the system window insets by default. -->
        <item name="consumeInsets">true</item>
    </style>

    <style name="DispatchFairInsetsRelativeLayout" parent="DispatchFairInsets" />

    <style name="DispatchFairInsetsFrameLayout" parent="DispatchFairInsets" />

    <style name="DispatchFairInsetsLinearLayout" parent="DispatchFairInsets" />

    <style name="ThumbnailSliderStyle">
        <item name="leftMenuItemColor">?attr/colorPrimary</item>
        <item name="rightMenuItemColor">?attr/colorPrimary</item>
        <item name="seekbarColor">?attr/colorPrimary</item>
    </style>

    <style name="ThumbnailSliderStyle.Seekbar" parent="Widget.AppCompat.SeekBar" >
        <item name="android:paddingTop">16dp</item>
        <item name="android:paddingBottom">16dp</item>
        <item name="android:layout_gravity">center</item>
         <item name="android:minHeight">2dp</item>
        <item name="android:maxHeight">2dp</item>
    </style>

    <style name="AnnotationToolbarStyle">
        <item name="colorBackground">?attr/colorPrimary</item>
        <item name="colorToolBackground">?attr/colorPrimaryDark</item>
        <item name="colorToolIcon">@android:color/white</item>
        <item name="colorCloseIcon">@android:color/white</item>
    </style>

    <style name="EditToolbarStyle">
        <item name="colorBackground">?android:attr/colorBackground</item>
        <item name="colorToolBackground">@color/controls_edit_toolbar_tool</item>
        <item name="colorToolIcon">?android:attr/colorForeground</item>
        <item name="colorCloseIcon">@color/controls_edit_toolbar_checkmark</item>
    </style>

    <style name="QuickMenuButton" parent="Widget.AppCompat.Button.Borderless">
        <item name="android:background">?attr/selectableItemBackground</item>
    </style>

    <style name="RectGroupAnnotEdit">
        <item name="fillColor">@color/tools_annot_edit_line_shadow</item>
        <item name="fillOpacity">0.38</item>
    </style>

    <style name="TabFragmentToolManager">
        <item name="edit_ink_annots">true</item>
        <item name="add_image_stamper_tool">true</item>
        <item name="open_toolbar_on_pan_ink_selected">true</item>
        <item name="build_in_page_number_indicator">false</item>
    </style>

    <style name="PresetColorGridViewStyle">
        <item name="color_list">@array/preset_colors</item>
        <item name="android:horizontalSpacing">@dimen/padding_xsmall</item>
        <item name="android:verticalSpacing">@dimen/padding_xsmall</item>
        <item name="android:stretchMode">columnWidth</item>
        <item name="android:numColumns">7</item>
        <item name="android:clipToPadding">false</item>
    </style>

    <style name="ColorPickerStyle">
        <item name="wheel_width">@dimen/quick_menu_button_size</item>
        <item name="value_pointer_radius">@dimen/padding_large</item>
        <item name="ring_color">@android:color/white</item>
        <item name="ring_shadow_radius">@dimen/action_button_min_width</item>
    </style>

    <style name="AnnotStyleBottomSheetDialogStyle" parent="Theme.Design.BottomSheetDialog">
        <item name="android:windowIsTranslucent">false</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowIsFloating">true</item>
    </style>

    <style name="FullScreenDialogStyle" parent="ThemeOverlay.AppCompat.Dialog.Alert">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@null</item>
        <item name="android:windowIsFloating">false</item>
        <item name="android:backgroundDimEnabled">false</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowNoTitle">true</item>
    </style>

    <style name="HighlightPresetStyle1">
        <item name="annot_color">#ffe6a2</item>
    </style>
    <style name="HighlightPresetStyle2">
        <item name="annot_color">#92e8e8</item>
